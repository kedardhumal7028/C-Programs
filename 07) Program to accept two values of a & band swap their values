#include<stdio.h>
#include<conio.h>
void main()
{
  int a,b,temp;
  clrscr();
  
  printf("Enter 1St number :");
  scanf("%d",&a)
  
  printf("Enter 2nd number :");
  scanf("%d",&b);
  
  printf("Before Swapping");
  printf("A=%d, B=%d",a,b);
  
  temp=a;
  a=b;
  b=temp;
  
  printf("\nAfter swapping");
  printf("\nA=%d, B=%d",a,b);
  getch();
}  
