#include<stdio.h>
struct student
{
  int RollNo;
  char Name[50];
  int Marks;
  char Grade[10];
};



int main()
{
    struct student a,b;

    printf("\nEnter Students RollNo :");
    scanf("%d%d",&a.RollNo,&b.RollNo);

    printf("\nEnter Students Name :");
    scanf("%s%s",a.Name,b.Name);

    printf("\nEnter Students Marks :");
    scanf("%d%d",&a.Marks,&b.Marks);

    printf("\nEnter Students Grade :");
    scanf("%s%s",&a.Grade,&b.Grade);

    printf("\nStudents %s Info :\n",a.Name);

    printf("\nRollNo = %d\nName = %s\nMarks = %d\nGrade = %s\n", a.RollNo, a.Name, a.Marks, a.Grade);

    printf("\nStudents %s Info :\n",b.Name);

    printf("\nRollNo = %d\nName = %s\nMarks = %d\nGrade = %s\n", b.RollNo, b.Name, b.Marks, b.Grade);
    return 0;

}


